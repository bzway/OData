@model FakeViewModel
@{
    ViewBag.Title = "Debug";
}

<h2>@ViewBag.Title.</h2>

@using (Html.BeginForm())
{

    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(m => m.token, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.token, new { @class = "form-control" })
        </div>
    </div>  
    
        <div class="form-group">
        @Html.Label("url", new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.nonce, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.data, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextAreaFor(m => m.data, new { @class = "form-control", @rows = 12 })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.result, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
         <label>@(Model.result)</label>    
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Debug" />
        </div>
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
